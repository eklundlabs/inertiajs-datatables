{
    "name": "eklundlabs/inertiajs-datatables",
    "description": "Laravel inertiajs datatables",
    "license": "MIT",
    "keywords": [
        "laravel",
        "inertia",
        "datatable"
    ],
    "authors": [
        {
            "name": "Kristoffer Eklund",
            "email": "kristoffer@eklundlabs.dev",
            "homepage": "https://eklundlabs.dev",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/eklundlabs/laravel-inertia-datatable",
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.2|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.16.1",
        "spatie/laravel-searchable": "^1.13"
    },
    "require-dev": {
        "larastan/larastan": "^2.7|^3.0",
        "mockery/mockery": "^1.6.7",
        "orchestra/testbench": "^8.36|^9.15|^10.8",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.3.1",
        "spatie/pest-expectations": "^1.13",
        "spatie/pest-plugin-snapshots": "^2.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Eklundlabs\\InertiaDatatable\\": "src",
            "Eklundlabs\\InertiaDatatable\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eklundlabs\\InertiaDatatable\\Tests\\": "tests",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eklundlabs\\InertiaDatatable\\InertiaDatatableProvider"
            ]
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "baseline": "vendor/bin/phpstan analyse --generate-baseline",
        "test": "vendor/bin/pest",
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "lint": [
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ]
    }
}